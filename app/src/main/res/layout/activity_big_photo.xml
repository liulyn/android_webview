<?xml version="1.0" encoding="utf-8"?>
<FrameLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:layout_width="match_parent"
    android:layout_height="match_parent" >

    <com.daxmall.m.util.photoview.GlidePhotoView.HackyViewPager
        android:id="@+id/viewpager"
        android:layout_width="match_parent"
        android:layout_height="match_parent" />
    
    <com.daxmall.m.util.photoview.GlidePhotoView.CircleIndicator
        android:id="@+id/circleIndicator"
        android:layout_width="match_parent"
        android:layout_height="15dp"
        app:ci_drawable="@drawable/checked_usb_open"
        app:ci_drawable_unselected="@drawable/checked_usb_close"
        android:layout_gravity="bottom"
        android:layout_marginBottom="30dp"
        />

</FrameLayout>
