<?xml version="1.0" encoding="utf-8"?>
<selector xmlns:android="http://schemas.android.com/apk/res/android">
    <!--定义按钮按下时的图片-->
    <item android:drawable="@drawable/shape_rectangle_round_click" android:state_selected="true" />
    <item android:drawable="@drawable/shape_rectangle_round_click" android:state_pressed="true" />
    <item android:drawable="@drawable/shape_rectangle_round_click" android:state_focused="true" />
    <item android:drawable="@drawable/shape_rectangle_round_enabled" android:state_enabled="false"/>
    <!--定义按钮默认的图片-->
    <item android:drawable="@drawable/shape_rectangle_round" />

</selector>
